#!groovy

node {
    checkout([$class: 'GitSCM', branches: [[name: '*/master']], 
     userRemoteConfigs: [[url: 'https://github.com/hvdbrand/Copol.git']]])
    echo  pwd()
    dir('CopolTest') {
		sh 'make'
    	sh './CopolTest --gtest_output=xml:gtestresults.xml'
    	sh 'scripts/awk_introduce_skipped'
    }
}